{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "yg-uoslib",
  "version": "0.0.1",
  "identifier": "io.github.yippeegadzooks.yg-uoslib",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "yg-uoslib",
        "width": 1280,
        "height": 1024
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources":[
      "config/**/*"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "store": null
  }

}
